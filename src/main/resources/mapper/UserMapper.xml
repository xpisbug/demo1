<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.smdemo1.dao.UserMapper2">
	<insert id="insert" parameterType="com.example.smdemo1.model.User">
		insert into user(`id`, `name`, `age`, `desc`)value(
			#{id},#{name},#{age},#{desc}
		)
	</insert>

	<select id="fetch" resultType="com.example.smdemo1.model.User">
		select * from user where id = #{id}
	</select>
</mapper>